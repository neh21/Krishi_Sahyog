
<div class="admin-title">
  <h1>KrishiSahyog</h>
  <h2>All Reports</h2>
  
</div>
<ul class="admin-posts"><% data.forEach(post => {  %>
 <li>
  <a href="/post/<%= post._id%>">
      <%= post.title%> &nearr;
      </a>
      <div class="admin-post-controls">
          <a href="/edit-post/<%= post._id%>" class="btn">Edit</a>
          <form action="/admin-delete-post/<%= post._id%>?_method=DELETE" method="POST" >
              <input type="submit" value="Delete" class="btn-delete btn">
              </form>
              <form action="/submit-post/<%= post._id%>" method="POST" >
                <input type="submit" value="Send" class="btn-delete btn">
                </form>
          </div>
  </li>
<% }); %></ul>